//Pipeline generated with command line: ./imb-pip.pl --tasks-pip=2 --force
//By: ssayolsp At: Di 11 Nov 2014 19:04:31 CET
executor="lsf"
queue="short"
commands {
	FastQC {
		lsf_request_options="-W2:00 -n1 -app Reserve2G -J FastQC"
	}
	bowtie_se {
		lsf_request_options="-W4:00 -R 'span[ptile=8]' -n8 -app Reserve2G -J bowtie"
	}
	BAMindexer {
		lsf_request_options="-W1:00 -n1 -app Reserve1G -J BAMindexer"
	}
	MarkDups {
		lsf_request_options="-W4:00 -n1 -app Reserve50G -J MarkDups"
	}
	extend {
		lsf_request_options="-W1:00 -n1 -app Reserve1G -J extend"
	}
	bam2bw {
		lsf_request_options="-W1:00 -n1 -app Reserve2G -J bam2bw"
	}
	ipstrength {
		lsf_request_options="-W1:00 -R 'span[ptile=2]' -n2 -app Reserve2G -J ipstrength"
	}
	phantompeak {
		lsf_request_options="-W1:00 -R 'span[ptile=8]' -n8 -app Reserve2G -J phantompeak"
	}
	pbc {
		lsf_request_options="-W1:00 -n1 -app Reserve4G -J pbc"
	}
	macs2 {
		queue="long"
		lsf_request_options="-W8:00 -n1 -app Reserve2G -J macs2"
	}
	collectBpipeLogs {
		lsf_request_options="-W0:05 -n1 -app Reserve300M -J collectBpipeLogs"
	}
	shinyReports {
		lsf_request_options="-W0:05 -n1 -app Reserve300M -J shinyReports"
	}
}
